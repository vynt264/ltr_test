{
  "name": "8B-dev-be-watermelon",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "lint:staged": "lint-staged",
    "exceljs": "^4.3.0",
    "fix": "yarn format && yarn lint:fix",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./src/system/config.system/data.source.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d ./src/system/config.system/data.source.ts migration:generate ./src/migrations/migration/$npm_config_name",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./src/migrations/migration/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./src/system/config.system/data.source.ts migration:revert",
    "typeorm:drop": "npm run typeorm -- schema:drop -d ./src/system/config.system/data.source.ts",
    "seed:run": "ts-node ./src/migrations/seeders/init.seed.ts"
  },
  "dependencies": {
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/microservices": "^9.4.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@types/cache-manager-ioredis": "^2.0.3",
    "@types/validator": "^13.7.10",
    "amqp-connection-manager": "^4.1.13",
    "amqplib": "^0.10.3",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.1.4",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.3.1",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-jest": "^27.1.6",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "husky": "^8.0.2",
    "i": "^0.3.7",
    "ioredis": "^5.2.4",
    "lint-staged": "10.5.3",
    "md5-typescript": "^1.0.5",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mysql2": "^3.5.0",
    "nest-winston": "^1.9.1",
    "nestjs-simple-redis-lock": "^1.0.1",
    "node-cron": "^3.0.2",
    "node-gyp": "^9.3.1",
    "node-rsa": "^1.1.1",
    "npm": "^9.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "triple-beam": "^1.3.0",
    "typeorm": "^0.3.15",
    "uuid": "^7.0.3",
    "winston": "^3.8.2",
    "xlsx": "^0.18.5",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/amqplib": "^0.10.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "@types/node-rsa": "^1.1.1",
    "@types/node-telegram-bot-api": "^0.61.6",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "@types/triple-beam": "^1.3.2",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^28.1.2",
    "moment": "^2.29.4",
    "node-telegram-bot-api": "^0.61.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "yarn prettier:fix",
      "yarn lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test"
    }
  }
}
